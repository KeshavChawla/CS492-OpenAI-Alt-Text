{
    "manifest_version": 3,
    "name": "Alt Text Filler",
    "description": "Fills alt text for images that are missing the alt tag",
    "version": "1.0",
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": "images/icon.png"
    },
    "content_scripts": [
        {
            "js": ["scripts/jquery.min.js", "scripts/content.js"],
            "matches": ["<all_urls>"]
        }
    ],
    "permissions": [
        "storage"
    ]
  }